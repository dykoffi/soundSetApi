{
    "name": "soundsetapi",
    "version": "1.0.0",
    "main": "./index.ts",
    "private": false,
    "scripts": {
        "test": "jest",
        "start:dev": "nodemon ./index.ts",
        "start:debug": "DEBUG=* nodemon ./index.ts",
        "db:update": "prisma migrate dev && cqx update",
        "db:start": "docker-compose up -d soundSetApi_db_dev",
        "compose:up": "docker-compose up -d soundSetApi",
        "docker:build": "cqx build && DOCKER_BUILDKIT=1 docker build -t dykoffi/soundsetapi ./build && rm -rdf build"
    },
    "dependencies": {
        "@google-cloud/storage": "^5.18.3",
        "@prisma/client": "3.11.1",
        "aws-sdk": "^2.1102.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cqx-secure": "^1.0.12",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "pm2": "^5.2.0",
        "prom-client": "^14.0.1",
        "serve-favicon": "^2.5.0",
        "swagger-ui-express": "^4.3.0"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/http-errors": "^1.8.2",
        "@types/jest": "^27.4.0",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/multer-s3": "^2.7.11",
        "@types/node": "^17.0.10",
        "@types/on-finished": "^2.3.1",
        "@types/serve-favicon": "^2.5.3",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "cqx": "^1.7.7",
        "eslint": "^8.7.0",
        "jest": "^27.4.4",
        "prisma": "^3.7.0",
        "prisma-json-schema-generator": "^1.6.0",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5",
        "webpack": "^5.67.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.test\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "author": {
        "email": "koffiedy@gmail.com",
        "name": "KOFFI Edy",
        "url": "https://www.linkedin.com/in/edy-koffi-948494192/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/author/soundSetApi.git"
    },
    "bugs": {
        "url": "https://github.com/author/soundSetApi/issues"
    },
    "homepage": "git+https://github.com/author/soundSetApi#readme",
    "license": "MIT"
}
